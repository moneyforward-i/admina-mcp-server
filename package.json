{
  "name": "@moneyforward_i/admina-mcp-server",
  "version": "1.0.1",
  "description": "An MCP server for Admina",
  "type": "module",
  "keywords": [
    "mcp",
    "mcp-server"
  ],
  "main": "./build/index.js",
  "bin": {
    "admina-mcp-server": "./build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc -w",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "inspector": "source .env && npx @modelcontextprotocol/inspector build/index.js -e ADMINA_API_KEY=$ADMINA_API_KEY -e ADMINA_ORGANIZATION_ID=$ADMINA_ORGANIZATION_ID",
    "test": "jest"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.1",
    "axios": "^1.8.4",
    "zod": "^3.24.3",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3"
  }
}
